!-   ===========  MATERIALS ===========

! --- EXTERIOR WALL: Brick veneer + steel stud + R-11 batt + gypboard ---
! Target assembly U ~ 0.37 W/m2-K (R-11 with steel stud thermal bridging)

  Material,
    Brick Veneer,            !- Name
    MediumRough,             !- Roughness
    0.1016,                  !- Thickness {m} (4 inches)
    0.89,                    !- Conductivity {W/m-K}
    1920,                    !- Density {kg/m3}
    790,                     !- Specific Heat {J/kg-K}
    0.9,                     !- Thermal Absorptance
    0.7,                     !- Solar Absorptance
    0.7;                     !- Visible Absorptance

  Material,
    R-11 Batt Insulation,   !- Name
    MediumSmooth,            !- Roughness
    0.09,                    !- Thickness {m} (3.5 inches)
    0.049,                   !- Conductivity {W/m-K} (accounts for steel stud bridging)
    12,                      !- Density {kg/m3}
    840,                     !- Specific Heat {J/kg-K}
    0.9,                     !- Thermal Absorptance
    0.5,                     !- Solar Absorptance
    0.5;                     !- Visible Absorptance

  Material,
    Gypsum Board,            !- Name
    MediumSmooth,            !- Roughness
    0.0159,                  !- Thickness {m} (5/8 inch)
    0.16,                    !- Conductivity {W/m-K}
    800,                     !- Density {kg/m3}
    1090,                    !- Specific Heat {J/kg-K}
    0.9,                     !- Thermal Absorptance
    0.5,                     !- Solar Absorptance
    0.5;                     !- Visible Absorptance

! --- ROOF: Built-up roof + R-15 insulation + concrete deck ---
! Target assembly R ~ 15 ft2-F-hr/Btu = R-2.64 m2-K/W

  Material,
    Roof Membrane,           !- Name
    VeryRough,               !- Roughness
    0.0095,                  !- Thickness {m}
    0.16,                    !- Conductivity {W/m-K}
    1120,                    !- Density {kg/m3}
    1460,                    !- Specific Heat {J/kg-K}
    0.9,                     !- Thermal Absorptance
    0.7,                     !- Solar Absorptance
    0.7;                     !- Visible Absorptance

  Material,
    Roof Insulation R15,     !- Name
    MediumRough,             !- Roughness
    0.110,                   !- Thickness {m} (~4.3 inches)
    0.042,                   !- Conductivity {W/m-K}
    32,                      !- Density {kg/m3}
    830,                     !- Specific Heat {J/kg-K}
    0.9,                     !- Thermal Absorptance
    0.5,                     !- Solar Absorptance
    0.5;                     !- Visible Absorptance

  Material,
    Concrete Deck,           !- Name
    MediumRough,             !- Roughness
    0.1016,                  !- Thickness {m} (4 inches)
    1.31,                    !- Conductivity {W/m-K}
    2240,                    !- Density {kg/m3}
    836,                     !- Specific Heat {J/kg-K}
    0.9,                     !- Thermal Absorptance
    0.6,                     !- Solar Absorptance
    0.6;                     !- Visible Absorptance

! --- FLOOR: Slab on grade (simplified) ---

  Material,
    Concrete Slab,           !- Name
    MediumRough,             !- Roughness
    0.2032,                  !- Thickness {m} (8 inches)
    1.31,                    !- Conductivity {W/m-K}
    2240,                    !- Density {kg/m3}
    836,                     !- Specific Heat {J/kg-K}
    0.9,                     !- Thermal Absorptance
    0.6,                     !- Solar Absorptance
    0.6;                     !- Visible Absorptance

! --- CEILING TILE ---

  Material,
    Ceiling Tile,            !- Name
    MediumSmooth,            !- Roughness
    0.019,                   !- Thickness {m}
    0.06,                    !- Conductivity {W/m-K}
    368,                     !- Density {kg/m3}
    590,                     !- Specific Heat {J/kg-K}
    0.9,                     !- Thermal Absorptance
    0.5,                     !- Solar Absorptance
    0.5;                     !- Visible Absorptance

  Material:AirGap,
    Wall Air Gap,            !- Name
    0.15;                    !- Thermal Resistance {m2-K/W}

  Material:AirGap,
    Ceiling Air Gap,         !- Name
    0.18;                    !- Thermal Resistance {m2-K/W}

!-   ===========  CONSTRUCTIONS ===========

  Construction,
    Exterior Wall,           !- Name
    Brick Veneer,            !- Outside Layer
    Wall Air Gap,            !- Layer 2
    R-11 Batt Insulation,   !- Layer 3
    Gypsum Board;            !- Layer 4

  Construction,
    Baseline Roof,           !- Name
    Roof Membrane,           !- Outside Layer
    Roof Insulation R15,     !- Layer 2
    Concrete Deck,           !- Layer 3
    Ceiling Air Gap,         !- Layer 4
    Ceiling Tile;            !- Layer 5

  Construction,
    Ground Floor,            !- Name
    Concrete Slab;           !- Outside Layer

  Construction,
    Interior Wall,           !- Name
    Gypsum Board,            !- Outside Layer
    Gypsum Board;            !- Inside Layer

  Construction,
    Interior Floor Ceiling,  !- Name
    Concrete Deck,           !- Outside Layer
    Ceiling Tile;            !- Inside Layer

! --- WINDOW: Double-pane clear, aluminum frame ---
! U-0.55 Btu/hr-ft2-F = 3.12 W/m2-K, SHGC 0.60

  WindowMaterial:SimpleGlazingSystem,
    Baseline Window Glazing, !- Name
    3.12,                    !- U-Factor {W/m2-K}
    0.60,                    !- Solar Heat Gain Coefficient
    0.70;                    !- Visible Transmittance

  Construction,
    Baseline Window,         !- Name
    Baseline Window Glazing; !- Outside Layer

!-   ===========  ZONE DEFINITIONS ===========
! Wing A Office: 35,000 ft2 = 3,252 m2 (2 floors)
!   Per floor: 1,626 m2 = ~40.3m x 40.3m (simplified as rectangle)
!   Actual: Long axis E-W, so use 50.4m x 32.3m per floor, 2.74m height (9ft)
!   Combined as single zone with double floor area and averaged height
!
! Wing B Library: 15,000 ft2 = 1,394 m2 (2 floors)
!   Mixed heights: 12ft reading, 9ft meeting. Use avg ~3.2m
!   ~26.4m x 26.4m per floor, combined
!
! Wing C Data Center: 2,000 ft2 = 186 m2 (single floor, interior)
!   ~13.6m x 13.6m, 2.74m height
!
! Common Areas: 10,000 ft2 = 929 m2
!   Lobby + corridors, ~30.5m x 15.2m avg, 3.5m (accounts for double-height lobby)

  Zone,
    WingA_Office,            !- Name
    0,                       !- Direction of Relative North {deg}
    0,                       !- X Origin {m}
    0,                       !- Y Origin {m}
    0,                       !- Z Origin {m}
    1,                       !- Type
    1,                       !- Multiplier
    ,                        !- Ceiling Height {m} (autocalculate)
    ,                        !- Volume {m3} (autocalculate)
    autocalculate,           !- Floor Area {m2}
    ,                        !- Zone Inside Convection Algorithm
    ,                        !- Zone Outside Convection Algorithm
    Yes;                     !- Part of Total Floor Area

  Zone,
    WingB_Library,           !- Name
    0,                       !- Direction of Relative North {deg}
    55,                      !- X Origin {m}
    0,                       !- Y Origin {m}
    0,                       !- Z Origin {m}
    1,                       !- Type
    1,                       !- Multiplier
    ,                        !- Ceiling Height {m}
    ,                        !- Volume {m3}
    autocalculate,           !- Floor Area {m2}
    ,                        !- Zone Inside Convection Algorithm
    ,                        !- Zone Outside Convection Algorithm
    Yes;                     !- Part of Total Floor Area

  Zone,
    WingC_DataCenter,        !- Name
    0,                       !- Direction of Relative North {deg}
    55,                      !- X Origin {m}
    30,                      !- Y Origin {m}
    0,                       !- Z Origin {m}
    1,                       !- Type
    1,                       !- Multiplier
    ,                        !- Ceiling Height {m}
    ,                        !- Volume {m3}
    autocalculate,           !- Floor Area {m2}
    ,                        !- Zone Inside Convection Algorithm
    ,                        !- Zone Outside Convection Algorithm
    Yes;                     !- Part of Total Floor Area

  Zone,
    CommonAreas,             !- Name
    0,                       !- Direction of Relative North {deg}
    0,                       !- X Origin {m}
    35,                      !- Y Origin {m}
    0,                       !- Z Origin {m}
    1,                       !- Type
