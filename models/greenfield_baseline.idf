!   N5=FanMotInAir, A5=ClgCoilType, A6=ClgCoilAvail, N6=ClgDesignSAT,
!   N7=ClgCoilCap, N8=ClgCoilSHR, N9=ClgCoilCOP, A7=HtgCoilType,
!   A8=HtgCoilAvail, N10=HtgDesignSAT, N11=HtgCoilCap, N12=GasHtgEff,
!   N13=GasHtgParElec, N14=MaxOAFlow, N15=MinOAFlow, A9=MinOASch,
!   A10=EconType, A11=EconLockout, N16=EconUpperT, N17=EconLowerT,
!   N18=EconUpperH, N19=EconMaxDP, A12=SupplyPlenum, A13=ReturnPlenum,
!   A14=FanPlacement, A15=NightCycleCtrl, ...
!
! Constant volume = fan operating mode schedule always 1 (continuous fan)
! This ensures fan runs at full speed whenever system is available
! =============================================

! Schedule for continuous fan operation (always on = constant volume)
  Schedule:Compact,
    Continuous Fan Mode,     !- Name
    Fraction,                !- Schedule Type Limits Name
    Through: 12/31,          !- Field 1
    For: AllDays,            !- Field 2
    Until: 24:00, 1.0;       !- Always continuous (1=on, 0=cycling)

! AHU-1: Wing A Office (15,000 CFM = 7.08 m3/s, 15HP = 11.19 kW)
! Fan delta pressure set high for CV system (~750 Pa = 3 inWG)
! HEATING: Gas coil → maintains gas vs OAT change-point for students
  HVACTemplate:System:Unitary,
    AHU-1 Office,            !- A1: Name
    Office HVAC Avail,       !- A2: System Availability Schedule Name
    WingA_Office,            !- A3: Control Zone Name
    7.08,                    !- N1: Supply Fan Maximum Flow Rate {m3/s} (15000 CFM)
    Continuous Fan Mode,     !- A4: Supply Fan Operating Mode Schedule Name (1=continuous=CV)
    0.60,                    !- N2: Supply Fan Total Efficiency
    750,                     !- N3: Supply Fan Delta Pressure {Pa} (3 inWG)
    0.85,                    !- N4: Supply Fan Motor Efficiency
    1.0,                     !- N5: Supply Fan Motor in Air Stream Fraction
    SingleSpeedDX,           !- A5: Cooling Coil Type
    ,                        !- A6: Cooling Coil Availability Schedule Name
    12.8,                    !- N6: Cooling Design Supply Air Temperature {C} (55F)
    autosize,                !- N7: Cooling Coil Gross Rated Total Capacity {W}
    autosize,                !- N8: Cooling Coil Gross Rated Sensible Heat Ratio
    3.5,                     !- N9: Cooling Coil Gross Rated COP (1995 recip chiller equiv)
    Gas,                     !- A7: Heating Coil Type (gas → preserves gas change-point)
    ,                        !- A8: Heating Coil Availability Schedule Name
    40.0,                    !- N10: Heating Design Supply Air Temperature {C} (104F)
    autosize,                !- N11: Heating Coil Capacity {W}
    0.82,                    !- N12: Gas Heating Coil Efficiency (82% boiler)
    0,                       !- N13: Gas Heating Coil Parasitic Electric Load {W}
    autosize,                !- N14: Maximum Outdoor Air Flow Rate {m3/s}
    autosize,                !- N15: Minimum Outdoor Air Flow Rate {m3/s}
    ,                        !- A9: Minimum Outdoor Air Schedule Name
    FixedDryBulb,            !- A10: Economizer Type
    ,                        !- A11: Economizer Lockout
    20.0,                    !- N16: Economizer Upper Temperature Limit {C} (68F)
    ,                        !- N17: Economizer Lower Temperature Limit {C}
    ,                        !- N18: Economizer Upper Enthalpy Limit {J/kg}
    ,                        !- N19: Economizer Maximum Limit Dewpoint Temperature {C}
    ,                        !- A12: Supply Plenum Name
    ,                        !- A13: Return Plenum Name
    DrawThrough,             !- A14: Supply Fan Placement
    CycleOnAny;              !- A15: Night Cycle Control

